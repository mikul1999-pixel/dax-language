{
  "name": "dax-language-syntax-m99",
  "displayName": "DAX Language Syntax vM99",
  "description": "DAX language support for Power BI",
  "version": "0.1.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/mikul1999-pixel/dax-language-syntax-m99.git"
  },
  "engines": {
    "vscode": "^1.85.0"
  },
  "categories": [
    "Programming Languages"
  ],
  "main": "./out/extension.js",
  "files": [
    "out",
    "syntaxes",
    "language-configuration.json",
    "package.json"
  ],
  "contributes": {
    "languages": [
      {
        "id": "dax",
        "aliases": [
          "DAX",
          "dax"
        ],
        "extensions": [
          ".dax"
        ],
        "configuration": "./language-configuration.json"
      }
    ],
    "grammars": [
      {
        "language": "dax",
        "scopeName": "source.dax",
        "path": "./syntaxes/dax.tmLanguage.json"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run build",
    "generate-grammar": "node generate-grammar.js",
    "build": "npm run generate-grammar && node esbuild.js",
    "watch": "node esbuild.js --watch",
    "pretest": "npm run build && npm run lint",
    "lint": "eslint src --ext ts"
  },
  "devDependencies": {
    "@types/node": "16.x",
    "@types/vscode": "^1.85.0",
    "@typescript-eslint/eslint-plugin": "^5.45.0",
    "@typescript-eslint/parser": "^5.45.0",
    "esbuild": "^0.27.2",
    "eslint": "^8.28.0",
    "typescript": "^4.9.3"
  }
}
