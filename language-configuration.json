{
    "comments": {
        // symbols used for single line comment
        "lineComment": "//",
        // symbols used for start and end block comment
        "blockComment": [ "/*", "*/" ]
    },
    // symbols used as brackets
    "brackets": [
        ["{", "}"],
        ["[", "]"],
        ["(", ")"]
    ],
    // symbols that are auto closed when typing
    "autoClosingPairs": [
        ["{", "}"],
        ["[", "]"],
        ["(", ")"],
        ["\"", "\""],
        ["'", "'"]
    ],
    // symbols that can be used to surround a selection
    "surroundingPairs": [
        ["{", "}"],
        ["[", "]"],
        ["(", ")"],
        ["\"", "\""],
        ["'", "'"]
    ],
    // Indent after VAR name = 
    // Outdent never ?
    "indentationRules": {
        "increaseIndentPattern": "^\\s*[Vv][Aa][Rr]\\b.*=\\s*$",
        "decreaseIndentPattern": "^\b$"
    }
}